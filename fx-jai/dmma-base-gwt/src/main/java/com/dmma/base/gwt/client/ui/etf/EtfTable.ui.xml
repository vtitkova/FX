<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:with field="cssRes"	type="com.dmma.base.gwt.client.resources.css.BaseCssResource" />
	 
	<g:HTMLPanel>
		<table width="100%" cellpadding="0" cellspacing="0">
			<tr>
				<td>
					<g:FlexTable ui:field="tableHeader" cellPadding="0" cellSpacing="0" width="100%" styleName="{cssRes.style.drawTableBorders}"/>	
				</td>
			</tr>
			<tr>
				<td>
					<g:ScrollPanel ui:field="scroller">
						<g:FlexTable ui:field="table" cellPadding="0" cellSpacing="0" width="100%" styleName="{cssRes.style.drawTableBorders}"/>
					</g:ScrollPanel>
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder>